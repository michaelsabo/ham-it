{
  "manifest_version": 2,

  "name": "Hamit",
  "description": "This extension adds gifs to ham.af",
  "version": "1.0",

  "browser_action": {
    "default_icon": "hamit.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://ham.life/*"
  ]
}
